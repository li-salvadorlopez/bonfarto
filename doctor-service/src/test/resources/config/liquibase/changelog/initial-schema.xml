<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet id="20170503041524-1" author="user">
        <createTable tableName="doctors">
            <column name="id" type="varchar(255)">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="first_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="last_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="created_by" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="created_date" type="datetime2">
                <constraints nullable="true" />
            </column>
            <column name="last_modified_by" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="last_modified_date" type="datetime2">
                <constraints nullable="true" />
            </column>
        </createTable>
    </changeSet>
    <changeSet id="20170503041524-2" author="user">
        <createTable tableName="doctor_addresses">
            <column name="id" type="varchar(255)">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="doctor_id" type="varchar(255)">
                <constraints foreignKeyName="DOCTOR_ADDRESSES_FK" nullable="false" />
            </column>
            <column name="street" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="city" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="state" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="zip_code" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="country" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="created_by" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="created_date" type="datetime2">
                <constraints nullable="true" />
            </column>
            <column name="last_modified_by" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="last_modified_date" type="datetime2">
                <constraints nullable="true" />
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>